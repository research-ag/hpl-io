type AggregatorAdminAPI = service {
  init : () -> (
    variant {
      err : variant { RegisterError };
      ok : nat; // stream id
    }
  );
  stats : () -> (Stats) query;
};

type Stats = record {
  canisterStatus : record {
    cyclesBalance : nat;
    memory_size : nat;
  };
  heartbeats : nat;
  txs : record { queued : nat };
};

service : () -> AggregatorAdminAPI;
